          "source": "factsheet",
          "field_path": "configuration/optimization_algorithm"
        }],
        "operation": "get_value",
        "type": "score_ranking",
        "direction": "asc",
        "score_map": {
          "FedAvg": 0.8493,
          "FedOpt": 0.8492,
          "FedProx": 0.8477,
          "FedBN": 0.8548,
          "pFedMe": 0.8765,
